<Window x:Class="InsertPhoto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InsertPhoto"
        mc:Ignorable="d"
        Title="Загрузка графика отпусков" Height="450" Width="800">
    <Grid>
        <Image Name="ImageService"/>

        <StackPanel Orientation="Vertical" VerticalAlignment="Bottom" HorizontalAlignment="Center" Height="85" Width="326">
            <Button Content="Выбрать изображение" Margin="10" Click="Button_Click" />
            <Button Content="Загрузить изображение" Margin="10" Click="Button_Click_1" />
        </StackPanel>
    </Grid>
</Window>
